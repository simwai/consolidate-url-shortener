{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "attach",
          "name": "Node: Nodemon",
          "processId": "${command:PickProcess}",
          "restart": true,
          "protocol": "inspector",
      },
      {
        "name": "ng serve",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200/#",
        "webRoot": "${workspaceFolder}/web-app/",
        "sourceMapPathOverrides": {
          "webpack:/*": "${webRoot}/*",
          "/src/*": "${webRoot}/src/*",
          "/*": "${webRoot}/*",
          // "/./*": "${webRoot}/*",
          // "/./~/*": "${webRoot}/wep-app/node_modules/*"
        }
      },
      {
        "name": "ng test",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:9876/debug.html",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "sourceMapPathOverrides": {
          "webpack:/*": "${webRoot}/wep-app/*",
          "/./*": "${webRoot}/web-app/*",
          "/src/*": "${webRoot}/web-app/*",
          "/*": "*/web-app",
          "/./~/*": "${webRoot}/wep-app/node_modules/*"
        }
      },
      {
        "name": "ng e2e",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/web-app/node_modules/protractor/bin/protractor",
        "protocol": "inspector",
        "args": ["${workspaceFolder}/web-app/e2e/protractor.conf.js"]
      }
  ]
}